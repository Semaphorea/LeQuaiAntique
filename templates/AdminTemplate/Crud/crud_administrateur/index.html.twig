{% extends 'base.html.twig' %}



{% block title %}Administrateur index{% endblock %}
{% block body %}

    <h2>Administrateur index</h2>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                
               
                <th>Username</th>
                <th>Password</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Email</th>
               {# <th>actions</th>#}
            </tr>
        </thead>
        <tbody>
        {% for administrateur in administrateurs %}

        
            <tr>
                <td>{{ administrateur.id }}</td>  
                <td>{{ administrateur.username }}</td>
                <td>{{ administrateur.password }}</td>
                <td></td>
                <td></td>
                <td>{{ administrateur.email }}</td>
                <td>
                    <a href="{{ path('app_crud_administrateur_show', {'id': administrateur.id}) }}">show</a>
                    <a href="{{ path('app_crud_administrateur_edit', {'id': administrateur.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_crud_administrateur_new') }}">Create new</a>
{% endblock %}


